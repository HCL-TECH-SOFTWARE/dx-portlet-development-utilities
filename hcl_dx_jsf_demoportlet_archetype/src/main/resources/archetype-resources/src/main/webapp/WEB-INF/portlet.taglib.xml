<?xml version='1.0' encoding='UTF-8'?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
   http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
	version="2.0">
	<namespace>http://java.sun.com/portlet_2_0</namespace>
	<tag>
		<description>Creates a URL that points to the current portlet and triggers a action request with the supplied parameters</description>
		<tag-name>actionURL</tag-name>
		<component>
			<component-type>com.ibm.faces20.portlet.component.PortletActionURL</component-type>
		</component>
		<attribute>
			<name>copyCurrentRenderParameters</name>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<name>escapeXml</name>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<name>name</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>portletMode</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>secure</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>var</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>windowState</name>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	<tag>
		<description>defines objects like RenderRequest,ResourceRequest,ActionRequest,EventRequest etc which are useful for portlet lifecycle.</description>
		<tag-name>defineObjects</tag-name>
		<component>
			<component-type>org.portletfaces.bridge.component.PortletDefineObjects</component-type>
		</component>
	</tag>
	<tag>
		<description>Creates a name space that will be unique for the current portlet resources</description>
		<tag-name>namespace</tag-name>
		<component>
			<component-type>com.ibm.faces20.portlet.component.PortletNameSpace</component-type>
		</component>
	</tag>
	<tag>
		<description>Creates a URL parameter that points to the current portlet and triggers a render request with the supplied parameters</description>
		<tag-name>param</tag-name>
		<component>
			<component-type>com.ibm.faces20.portlet.component.PortletParam</component-type>
		</component>
		<attribute>
			<name>name</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>value</name>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	<tag>
		<description>Creates a URL that points to the current portlet and triggers a render request with the supplied parameters</description>
		<tag-name>renderURL</tag-name>
		<component>
			<component-type>com.ibm.faces20.portlet.component.PortletRenderURL</component-type>
		</component>
		<attribute>
			<name>copyCurrentRenderParameters</name>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<name>escapeXml</name>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<name>portletMode</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>secure</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>var</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>windowState</name>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	<tag>
		<tag-name>resourceURL</tag-name>
		<component>
			<component-type>com.ibm.faces20.portlet.component.PortletResourceURL</component-type>
		</component>
		<attribute>
			<name>cacheability</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>escapeXml</name>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<name>id</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>secure</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>var</name>
			<type>java.lang.String</type>
		</attribute>
	</tag>
</facelet-taglib>