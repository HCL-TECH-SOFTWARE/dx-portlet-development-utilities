<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN"
  "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">
<taglib>
    <tlibversion>1.0</tlibversion>
    <jspversion>1.1</jspversion>
    <shortname>Tags for portlets</shortname>
    <tag>
        <name>client</name>
        <tagclass>com.ibm.wps.pe.pc.legacy.tags.ClientTag</tagclass>
        <teiclass>com.ibm.wps.pe.pc.legacy.tags.ClientTag$TEI</teiclass>
        <bodycontent>empty</bodycontent>
    </tag>
    <tag>
        <name>createReturnURI</name>
        <tagclass>com.ibm.wps.pe.pc.legacy.tags.CreateReturnURITag</tagclass>
        <bodycontent>JSP</bodycontent>
    </tag>
    <tag>
        <name>createURI</name>
        <tagclass>com.ibm.wps.pe.pc.legacy.tags.CreateURITag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>state</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>dataAttribute</name>
        <tagclass>com.ibm.wps.pe.pc.legacy.tags.DataAttributeTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>name</name>
            <required>false</required>
        </attribute>
    </tag>
    <tag>
        <name>dataLoop</name>
        <tagclass>com.ibm.wps.pe.pc.legacy.tags.DataLoopTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>pattern</name>
            <required>false</required>
        </attribute>
    </tag>
    <tag>
        <name>encodeNamespace</name>
        <tagclass>com.ibm.wps.pe.pc.legacy.tags.EncodeNamespaceTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>value</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>encodeURI</name>
        <tagclass>com.ibm.wps.pe.pc.legacy.tags.EncodeURITag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>path</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>bidi</name>
        <tagclass>com.ibm.wps.engine.tags.BidiTag</tagclass>
        <bodycontent>JSP</bodycontent>
	  <attribute>
            <name>locale</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>        
	  <attribute>
            <name>dir</name>
            <required>false</required>
        </attribute>
<!-- The 'is' attribute is deprecated, please use the 'dir' attribute -->  
	  <attribute>
            <name>is</name>
            <required>false</required>
        </attribute>        	  
    </tag>

    <tag>
        <name>if</name>
        <tagclass>com.ibm.wps.pe.pc.legacy.tags.IfTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>mode</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>previousMode</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>state</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>locale</name>
            <required>false</required>
        </attribute>
	  <attribute>
            <name>markup</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>mimetype</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>capability</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>init</name>
        <tagclass>com.ibm.wps.pe.pc.legacy.tags.InitTag</tagclass>
        <teiclass>com.ibm.wps.pe.pc.legacy.tags.InitTag$TEI</teiclass>
        <bodycontent>empty</bodycontent>
    </tag>
    <tag>
        <name>log</name>
        <tagclass>com.ibm.wps.pe.pc.legacy.tags.LogTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>text</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>level</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>settingsAttribute</name>
        <tagclass>com.ibm.wps.pe.pc.legacy.tags.SettingsAttributeTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>name</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>settingsLoop</name>
        <tagclass>com.ibm.wps.pe.pc.legacy.tags.SettingsLoopTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>pattern</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>text</name>
        <tagclass>com.ibm.wps.pe.pc.legacy.tags.TextTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>key</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>bundle</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>URIAction</name>
        <tagclass>com.ibm.wps.pe.pc.legacy.tags.URIActionTag</tagclass>
        <bodycontent>empty</bodycontent>
        <attribute>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>URIParameter</name>
        <tagclass>com.ibm.wps.pe.pc.legacy.tags.URIParameterTag</tagclass>
        <bodycontent>empty</bodycontent>
        <attribute>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>value</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
</taglib>
